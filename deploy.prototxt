    lr_mult: 2
    decay_mult: 0
  }
  convolution_param {
    axis: 1
    num_output: 64
    pad: 1
    kernel_size: 3
    kernel_size: 3
    kernel_size: 3
    weight_filler {
      type: "gaussian"
      std: 0.01
    }
    bias_filler {
      type: "constant"
      std: 0
    }
     
  }
}
layer {
   bottom: "conv2b"
